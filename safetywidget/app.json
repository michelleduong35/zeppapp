{
  "configVersion": "v3",
  "app": {
    "appId": 1038092,
    "appName": "Safety Widget",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.0.1"
    },
    "icon": "icon.png",
    "vender": "zepplins",
    "description": "app for safety notification and suggested open routes"
  },
  "permissions": [
    "data:os.device.info",
    "device:os.local_storage",
    "device:os.geolocation"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "3.0.0",
      "target": "3.0.0",
      "minVersion": "3.0"
    }
  },
  "debug": false,
  "targets": {
    "gt": {
      "module": {
        "page": {
          "pages": ["page/gt/home/index.page", "page/gt/home/map.page"]
        }
      },
      "platforms": [
        {
          "st": "r"
        },
        {
          "st": "s"
        }
      ],
      "designWidth": 480
    }
  },
  "i18n": {
    "en-US": {
      "appName": "Safety Widget"
    }
  },
  "defaultLanguage": "en-US"
}
